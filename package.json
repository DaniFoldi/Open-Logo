{
  "name": "Open-Logo",
  "version": "0.1.0",
  "description": "A Logo port to JavaScript",
  "repository": "https://github.com/DaniFoldi/Open-Logo.git",
  "author": "Dániel Földi <dani@stefan.hu>",
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-builder": "^20.38.0",
    "eslint": "^5.9.0",
    "eslint-plugin-html": "^5.0.0",
    "standard": "^12.0.1"
  },
  "standard": {
    "env": [
      "jquery"
    ],
    "plugins": [
      "html"
    ]
  },
  "dependencies": {
